@page
@model Dawson_HW4.Pages.Categories.CannedFoodsModel

<h1>Product List</h1>

<div class="row">
    @foreach (var product in Model.ProdList)
    {
        <div class="col-md-4 mb-3">
            <a href="#" class="card h-100 text-decoration-none text-dark">
                <img src="~/fruits.jpg" class="card-img-top" alt="@product.ProductName" />
                <div class="card-body">
                    <h5 class="card-title">@product.ProductName</h5>
                </div>
            </a>
        </div>
    }
</div>
